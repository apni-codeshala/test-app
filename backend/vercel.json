{
    "version": 2,
    "builds": [
        {
            "src": "src/routes/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "src": "/api/create", "dest": "src/routes/index.js" },
        { "src": "/api/getall", "dest": "src/routes/index.js" },
        { "src": "/api/getbyid/:id", "dest": "src/routes/index.js" },
        { "src": "/api/delete/:id", "dest": "src/routes/index.js" },
        { "src": "/api/update", "dest": "src/routes/index.js" }
    ]
}
